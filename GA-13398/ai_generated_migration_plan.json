{
    "Migration Plan": [
        {
            "step": "create_dataiku_project",
            "parameters": {
                "project_key": "INSURANCE_DATA_INTEGRATION",
                "name": "Insurance Data Integration and Customer Insights",
                "description": "This project enhances insurance operations and customer insights through strategic data integration and transformation.",
                "owner": "admin"
            }
        },
        {
            "step": "create_s3_file_datasets",
            "parameters": {
                "project_key": "INSURANCE_DATA_INTEGRATION",
                "connection_name": "Customer360Connection",
                "bucket_name": "dataeconomy-ohio",
                "base_path": "ApplicationsData/JNJDataverse/Customer360/",
                "file_names": [
                    "aiml_insights.xlsx",
                    "claims.csv",
                    "demographics.xlsx",
                    "policy.xlsx",
                    "scores.xlsx"
                ]
            }
        },
        {
            "step": "create_prepare_recipe",
            "parameters": {
                "project_key": "INSURANCE_DATA_INTEGRATION",
                "recipe_name": "prepare_demographics",
                "input_dataset": "demographics.xlsx",
                "output_dataset": "prepared_demographics",
                "actions": [
                    {
                        "action": "SelectColumns",
                        "params": {
                            "keep": [
                                "Customer_ID",
                                "Customer_Name",
                                "Email",
                                "Phone_Number",
                                "Address",
                                "City",
                                "State",
                                "Postal_Code",
                                "Date_of_Birth",
                                "Gender",
                                "Marital_Status",
                                "Occupation",
                                "Income_Level",
                                "Customer_Segment"
                            ]
                        }
                    }
                ]
            }
        },
        {
            "step": "create_prepare_recipe",
            "parameters": {
                "project_key": "INSURANCE_DATA_INTEGRATION",
                "recipe_name": "prepare_claims",
                "input_dataset": "claims.csv",
                "output_dataset": "prepared_claims",
                "actions": [
                    {
                        "action": "SelectColumns",
                        "params": {
                            "keep": [
                                "Claim_ID",
                                "Policy_ID",
                                "Claim_Date",
                                "Claim_Type",
                                "Claim_Status",
                                "Claim_Amount",
                                "Claim_Payout"
                            ]
                        }
                    }
                ]
            }
        },
        {
            "step": "create_prepare_recipe",
            "parameters": {
                "project_key": "INSURANCE_DATA_INTEGRATION",
                "recipe_name": "prepare_policy",
                "input_dataset": "policy.xlsx",
                "output_dataset": "prepared_policy",
                "actions": [
                    {
                        "action": "SelectColumns",
                        "params": {
                            "keep": [
                                "Policy_ID",
                                "Customer_ID",
                                "Policy_Type",
                                "Policy_Status",
                                "Policy_Start_Date",
                                "Policy_End_Date",
                                "Policy_Term",
                                "Policy_Premium",
                                "Total_Premium_Paid",
                                "Renewal_Status",
                                "Policy_Addons"
                            ]
                        }
                    }
                ]
            }
        },
        {
            "step": "create_join_recipe",
            "parameters": {
                "project_key": "INSURANCE_DATA_INTEGRATION",
                "recipe_name": "join_demographics_policy",
                "left_input": "prepared_demographics",
                "right_input": "prepared_policy",
                "output_dataset": "joined_demographics_policy",
                "join_type": "INNER",
                "join_conditions": [
                    {
                        "left_column": "Customer_ID",
                        "right_column": "Customer_ID",
                        "operator": "EQ"
                    }
                ]
            }
        },
        {
            "step": "create_join_recipe",
            "parameters": {
                "project_key": "INSURANCE_DATA_INTEGRATION",
                "recipe_name": "join_claims",
                "left_input": "joined_demographics_policy",
                "right_input": "prepared_claims",
                "output_dataset": "joined_all_data",
                "join_type": "INNER",
                "join_conditions": [
                    {
                        "left_column": "Policy_ID",
                        "right_column": "Policy_ID",
                        "operator": "EQ"
                    }
                ]
            }
        },
        {
            "step": "run_aggregation_flow",
            "parameters": {
                "project_key": "INSURANCE_DATA_INTEGRATION",
                "input_dataset": "joined_all_data",
                "output_dataset": "aggregated_data",
                "group_by": [
                    "Customer_ID"
                ],
                "aggregations": [
                    {
                        "column": "Claim_ID",
                        "aggregation_type": "count",
                        "output_column": "Total_Claims"
                    },
                    {
                        "column": "Claim_Date",
                        "aggregation_type": "max",
                        "output_column": "Recent_Claim_Date"
                    },
                    {
                        "column": "Claim_Amount",
                        "aggregation_type": "avg",
                        "output_column": "Average_Claim_Amount"
                    },
                    {
                        "column": "Policy_ID",
                        "aggregation_type": "count",
                        "output_column": "Policy_Count"
                    }
                ]
            }
        },
        {
            "step": "generate_and_create_dataiku_code_recipe",
            "parameters": {
                "project_key": "INSURANCE_DATA_INTEGRATION",
                "input_dataset_name": "aggregated_data",
                "output_dataset_name": "final_customer_insights",
                "recipe_name": "calculate_metrics",
                "run_recipe": true,
                "transformations": [
                    {
                        "action": "add_column",
                        "parameters": {
                            "column_name": "Age",
                            "formula": "DateTimeDiff(DateTimeToday(), Date_of_Birth, 'years')"
                        }
                    },
                    {
                        "action": "add_column",
                        "parameters": {
                            "column_name": "Claim_To_Premium_Ratio",
                            "formula": "if (Total_Premium_Paid != 0, Claim_Amount / Total_Premium_Paid, 0)"
                        }
                    },
                    {
                        "action": "add_column",
                        "parameters": {
                            "column_name": "Claims_Per_Policy",
                            "formula": "if (Policy_Count != 0, Total_Claims / Policy_Count, 0)"
                        }
                    },
                    {
                        "action": "add_column",
                        "parameters": {
                            "column_name": "Retention_Rate",
                            "formula": "0.85"
                        }
                    },
                    {
                        "action": "add_column",
                        "parameters": {
                            "column_name": "Cross_Sell_Opportunities",
                            "formula": "'Multi-Policy Discount, Home Coverage Add-on'"
                        }
                    },
                    {
                        "action": "add_column",
                        "parameters": {
                            "column_name": "Upsell_Potential",
                            "formula": "'Premium Vehicle Coverage'"
                        }
                    }
                ]
            }
        },
        {
            "step": "create_and_run_stack_recipe",
            "parameters": {
                "input_datasets": [
                    "final_customer_insights",
                    "aiml_insights.xlsx",
                    "scores.xlsx"
                ],
                "output_dataset": "Customer_360",
                "connection": "filesystem_managed",
                "output_format": "CSV",
                "project_key": "INSURANCE_DATA_INTEGRATION"
            }
        }
    ]
}