    {
        "step": "create_recipe",
        "parameters": {
            "project_key": "INSURANCE_CUSTOMER_360",
            "recipe_type": "Prepare",
            "input_dataset": "Demographics Data",
            "output_dataset": "Prepared Demographics Data",
            "actions": [
                {
                    "action": "select_columns",
                    "parameters": {
                        "columns": [
                            "Customer_ID",
                            "Customer_Name",
                            "Email",
                            "Phone_Number",
                            "Address",
                            "City",
                            "State",
                            "Postal_Code",
                            "Date_of_Birth",
                            "Gender",
                            "Marital_Status",
                            "Occupation",
                            "Income_Level",
                            "Customer_Segment"
                        ]
                    }
                }
            ]
        }
    },
    {
        "step": "create_recipe",
        "parameters": {
            "project_key": "INSURANCE_CUSTOMER_360",
            "recipe_type": "Prepare",
            "input_dataset": "Claims Data",
            "output_dataset": "Prepared Claims Data",
            "actions": [
                {
                    "action": "select_columns",
                    "parameters": {
                        "columns": [
                            "Claim_ID",
                            "Policy_ID",
                            "Claim_Date",
                            "Claim_Type",
                            "Claim_Status",
                            "Claim_Amount",
                            "Claim_Payout"
                        ]
                    }
                }
            ]
        }
    },
    {
        "step": "create_recipe",
        "parameters": {
            "project_key": "INSURANCE_CUSTOMER_360",
            "recipe_type": "Prepare",
            "input_dataset": "Policy Data",
            "output_dataset": "Prepared Policy Data",
            "actions": [
                {
                    "action": "select_columns",
                    "parameters": {
                        "columns": [
                            "policy_id",
                            "customer_id",
                            "policy_type",
                            "policy_status",
                            "policy_start_date",
                            "policy_end_date",
                            "policy_term",
                            "policy_premium",
                            "total_premium_paid",
                            "renewal_status",
                            "policy_addons"
                        ]
                    }
                }
            ]
        }
    },
    {
        "step": "create_recipe",
        "parameters": {
            "project_key": "INSURANCE_CUSTOMER_360",
            "recipe_type": "Join",
            "input_datasets": [
                "Prepared Demographics Data",
                "Prepared Policy Data"
            ],
            "output_dataset": "Joined Demographics and Policy Data",
            "join_type": "inner",
            "join_keys": [
                {
                    "left_key": "Customer_ID",
                    "right_key": "customer_id"
                }
            ]
        }
    },
    {
        "step": "create_recipe",
        "parameters": {
            "project_key": "INSURANCE_CUSTOMER_360",
            "recipe_type": "Join",
            "input_datasets": [
                "Joined Demographics and Policy Data",
                "Prepared Claims Data"
            ],
            "output_dataset": "Joined All Data",
            "join_type": "inner",
            "join_keys": [
                {
                    "left_key": "policy_id",
                    "right_key": "Policy_ID"
                }
            ]
        }
    },
    {
        "step": "create_recipe",
        "parameters": {
            "project_key": "INSURANCE_CUSTOMER_360",
            "recipe_type": "Group",
            "input_dataset": "Joined All Data",
            "output_dataset": "Aggregated Data",
            "group_by": [
                "Customer_ID"
            ],
            "aggregations": [
                {
                    "column": "Claim_ID",
                    "type": "count",
                    "output_column": "Total_Claims"
                },
                {
                    "column": "policy_id",
                    "type": "count",
                    "output_column": "Policy_Count"
                },
                {
                    "column": "Claim_Date",
                    "type": "max",
                    "output_column": "Recent_Claim_Date"
                },
                {
                    "column": "Claim_Amount",
                    "type": "avg",
                    "output_column": "Average_Claim_Amount"
                }
            ]
        }
    },
    {
        "step": "create_recipe",
        "parameters": {
            "project_key": "INSURANCE_CUSTOMER_360",
            "recipe_type": "Join",
            "input_datasets": [
                "Aggregated Data",
                "Joined All Data"
            ],
            "output_dataset": "Final Joined Data",
            "join_type": "inner",
            "join_keys": [
                {
                    "left_key": "Customer_ID",
                    "right_key": "Customer_ID"
                }
            ]
        }
    },
    {
        "step": "create_recipe",
        "parameters": {
            "project_key": "INSURANCE_CUSTOMER_360",
            "recipe_type": "Prepare",
            "input_dataset": "Final Joined Data",
            "output_dataset": "Calculated Metrics Data",
            "actions": [
                {
                    "action": "add_column",
                    "parameters": {
                        "column_name": "Age",
                        "formula": "DateTimeDiff(DateTimeToday(), [Date_of_Birth], 'years')"
                    }
                },
                {
                    "action": "add_column",
                    "parameters": {
                        "column_name": "Claim_To_Premium_Ratio",
                        "formula": "if([total_premium_paid] != 0, [Claim_Amount]/[total_premium_paid], 0)"
                    }
                },
                {
                    "action": "add_column",
                    "parameters": {
                        "column_name": "Claims_Per_Policy",
                        "formula": "if([Policy_Count] != 0, [Total_Claims]/[Policy_Count], 0)"
                    }
                },
                {
                    "action": "add_column",
                    "parameters": {
                        "column_name": "Retention_Rate",
                        "formula": "0.85"
                    }
                },
                {
                    "action": "add_column",
                    "parameters": {
                        "column_name": "Cross_Sell_Opportunities",
                        "formula": "'Multi-Policy Discount, Home Coverage Add-on'"
                    }
                },
                {
                    "action": "add_column",
                    "parameters": {
                        "column_name": "Upsell_Potential",
                        "formula": "'Premium Vehicle Coverage'"
                    }
                }
            ]
        }
    },
    {
        "step": "create_recipe",
        "parameters": {
            "project_key": "INSURANCE_CUSTOMER_360",
            "recipe_type": "Prepare",
            "input_dataset": "AI/ML Insights",
            "output_dataset": "Prepared AIML Insights Data",
            "actions": [
                {
                    "action": "select_columns",
                    "parameters": {
                        "columns": [
                            "Customer_ID",
                            "Churn_Probability",
                            "Next_Best_Offer",
                            "Claims_Fraud_Probability",
                            "Revenue_Potential"
                        ]
                    }
                }
            ]
        }
    },
    {
        "step": "create_recipe",
        "parameters": {
            "project_key": "INSURANCE_CUSTOMER_360",
            "recipe_type": "Prepare",
            "input_dataset": "Scores Data",
            "output_dataset": "Prepared Scores Data",
            "actions": [
                {
                    "action": "select_columns",
                    "parameters": {
                        "columns": [
                            "Customer_ID",
                            "Credit_Score",
                            "Fraud_Score",
                            "Customer_Risk_Score"
                        ]
                    }
                }
            ]
        }
    },
    {
        "step": "create_recipe",
        "parameters": {
            "project_key": "INSURANCE_CUSTOMER_360",
            "recipe_type": "Join",
            "input_datasets": [
                "Calculated Metrics Data",
                "Prepared AIML Insights Data",
                "Prepared Scores Data"
            ],
            "output_dataset": "Customer 360 Data",
            "join_type": "inner",
            "join_keys": [
                {
                    "left_key": "Customer_ID",
                    "right_key": "Customer_ID"
                }
            ]
        }
    }
]